pipeline{
    tools
	{
        jdk 'myjava'
        maven 'mymaven'
    }
	agent any
      stages
	  {
           stage('Checkout'){
	    
               steps{
		 echo 'cloning..'
                 git 'https://github.com/PraveenB1712/samplejavaapp.git'
              }
          }
          stage('Compile'){
             
              steps{
                  echo 'complie the code..'
                  sh 'mvn compile'
	      }
          }
          stage('CodeReview'){
		  
              steps{
		    
		  echo 'codeReview'
                  sh 'mvn pmd:pmd'
              }
          }
           stage('UnitTest'){
		  
		   
              steps{
	         
                  sh 'mvn test'
              }
          
          }
        
          stage('Package'){
		  
              steps{
		       sh 'mvn package'
		       
             //   sonar.projectKey=webapp1
               //sonar.ProjectName=webapp1
               //sonar.sources=src/main/java/
               //sonar.java.binaries=target/classes/
                //sonar.language=java
                //sonar.java.source=1.8
 
//sh "mvn sonar:sonar -Dsonar.login=${env.sonarqube}"
           //      sh 'mvn sonar:sonar \
             //     -Dsonar.projectKey=webapp1 \
               //   -Dsonar.host.url=http://34.125.69.253:9000 \
                 // -Dsonar.login=${env.}'
		 
		   withSonarQubeEnv('sonarqube') {
                 sh 'mvn clean package sonar:sonar'
              }
              }
          }
		  
         
		 
    
}
}
